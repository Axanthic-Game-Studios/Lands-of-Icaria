{
  "multipart": [
    {
      "when": { "up": "true" },
      "apply": { "model": "landsoficaria:block/baetyl_cobble_wall_post", "y": 0, "uvlock": false }
    },
    {
      "when": { "north": "low" },
      "apply": { "model": "landsoficaria:block/baetyl_cobble_wall_low", "y": 0, "uvlock": true }
    },
    {
      "when": { "east": "low" },
      "apply": { "model": "landsoficaria:block/baetyl_cobble_wall_low", "y": 90, "uvlock": true }
    },
    {
      "when": { "south": "low" },
      "apply": { "model": "landsoficaria:block/baetyl_cobble_wall_low", "y": 180, "uvlock": true }
    },
    {
      "when": { "west": "low" },
      "apply": { "model": "landsoficaria:block/baetyl_cobble_wall_low", "y": 270, "uvlock": true }
    },
    {
      "when": { "north": "tall" },
      "apply": { "model": "landsoficaria:block/baetyl_cobble_wall_tall", "y": 0, "uvlock": true }
    },
    {
      "when": { "east": "tall" },
      "apply": { "model": "landsoficaria:block/baetyl_cobble_wall_tall", "y": 90, "uvlock": true }
    },
    {
      "when": { "south": "tall" },
      "apply": { "model": "landsoficaria:block/baetyl_cobble_wall_tall", "y": 180, "uvlock": true }
    },
    {
      "when": { "west": "tall" },
      "apply": { "model": "landsoficaria:block/baetyl_cobble_wall_tall", "y": 270, "uvlock": true }
    }
  ]
}